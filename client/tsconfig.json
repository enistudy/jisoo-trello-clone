{
	"compilerOptions": {
		"outDir": "./build/",
		"sourceMap": true,
		"noImplicitAny": true,
		"module": "commonjs",
		"target": "es5",
		"jsx": "react",
		"baseUrl": "./src",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"strict": true,
		"typeRoots": ["src/@types", "./node_modules/@types"]
	},
	"include": ["./src/**/*"],
	"exclude": ["./node_modules/**/*"]
}
